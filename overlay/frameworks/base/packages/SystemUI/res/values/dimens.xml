<resources>
    <!-- for 20dp of padding at 3.5px/dp at default density -->
    <dimen name="rounded_corner_content_padding">16px</dimen>

    <!-- Margin on the left side of the carrier text on Keyguard -->
    <dimen name="keyguard_carrier_text_margin">25dp</dimen>

    <!-- Margin on the right side of the system icon group on Keyguard. -->
    <dimen name="system_icons_keyguard_padding_end">8dp</dimen>

    <!-- Location on the screen of the center of the physical power button. This is a reasonable
    default that should be overridden by device-specific overlays. -->
    <dimen name="physical_power_button_center_screen_location_y">@dimen/physical_fingerprint_sensor_center_screen_location_y</dimen>

    <!-- Power/fps button location -->
    <dimen name="physical_power_button_center_screen_location_y">975px</dimen>
    <dimen name="physical_fingerprint_sensor_center_screen_location_y">975px</dimen>
    <dimen name="physical_fingerprint_sensor_center_screen_location_x">1080px</dimen>
